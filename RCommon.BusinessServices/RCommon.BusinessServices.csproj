<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIcon>reactor2-icon-darkbg-small.jpg</PackageIcon>
    <PackageIconUrl />
    <PackageProjectUrl>https://github.com/Reactor2Team/RCommon</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Reactor2Team/RCommon</RepositoryUrl>
    <Description>A lightweight application framework that utilizes abstractions over commonly used patterns to future proof applications.</Description>
    <Authors>Reactor2, Jason Webb, Ritesh Rao</Authors>
    <Company>RCommon</Company>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>RCommon, business services, business service abstractions, CRUD helpers, c#, .NET</PackageTags>
    <Version>1.0.1.40</Version>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\RCommon.BusinessEntities\RCommon.BusinessEntities.csproj" />
    <ProjectReference Include="..\RCommon.DataServices\RCommon.DataServices.csproj" />
    <ProjectReference Include="..\RCommon.ExceptionHandling\RCommon.ExceptionHandling.csproj" />
    <ProjectReference Include="..\RCommon.Models\RCommon.Models.csproj" />
    <ProjectReference Include="..\RCommon.Persistence\RCommon.Persistence.csproj" />
    <ProjectReference Include="..\RCommon.Validation\RCommon.Validation.csproj" />
    <ProjectReference Include="..\RCommon\RCommon.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\RCommon\reactor2-icon-darkbg-small.jpg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
