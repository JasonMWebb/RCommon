<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.1.42</Version>
    <Authors>Reactor2, Jason Webb, Ritesh Rao</Authors>
    <Description>A lightweight application framework that utilizes abstractions over commonly used patterns to future proof applications.</Description>
    <PackageProjectUrl>https://github.com/Reactor2Team/RCommon</PackageProjectUrl>
    <PackageIcon>reactor2-icon-darkbg-small.jpg</PackageIcon>
    <PackageIconUrl />
    <RepositoryUrl>https://github.com/Reactor2Team/RCommon</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>RCommon, c#, .NET, background jobs, background workers, background job abstractions</PackageTags>
    <Company>RCommon</Company>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="2021.3.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\RCommon.ExceptionHandling\RCommon.ExceptionHandling.csproj" />
    <ProjectReference Include="..\RCommon.Core\RCommon.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\RCommon.Core\reactor2-icon-darkbg-small.jpg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
