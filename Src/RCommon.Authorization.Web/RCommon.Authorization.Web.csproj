<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Company>Reactor2, Jason Webb</Company>
    <Description>A lightweight application framework that utilizes abstractions over commonly used patterns to future proof applications.</Description>
    <PackageProjectUrl>https://github.com/Reactor2Team/RCommon</PackageProjectUrl>
    <PackageIcon>reactor2-icon-darkbg-small.jpg</PackageIcon>
    <RepositoryUrl>https://github.com/Reactor2Team/RCommon</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>RCommon, Authorization, Web, Framework</PackageTags>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Version>1.0.1.41</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\RCommon.Core\reactor2-icon-darkbg-small.jpg">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Swashbuckle.AspNetCore.SwaggerGen" Version="6.2.3" />
  </ItemGroup>

</Project>
